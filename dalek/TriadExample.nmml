<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<app title="TriadNME" file="TriadNME" main="Main" package="com.ludamix.triad.app" version="1.0.0" company="Ludamix" />
	
	<window width="640" height="400" fps="20" orientation="portrait" resizable="true" if="target_flash" />
	<window width="640" height="400" fps="20" orientation="portrait" resizable="false" unless="target_flash" />
 	
	<set name="BUILD_DIR" value="Export" />
	<set name="SWF_VERSION" value="11.0" />
	
	<haxedef name="fdb" if="target_flash" />
	<!--<haxedef name="layout_debug" if="target_flash" /> !-->
	
	<classpath name="Source" />
	
	<haxelib name="nme" />
	<haxelib name="hxJson2" />
	<haxelib name="format" />
	<haxelib name="triad" />
	<haxelib name="hscript" />
	
	<assets path="Assets" rename="assets" include="*" exclude="nme.svg" />
	
	<icon name="Assets/nme.svg" />
	
	<ndll name="std" />
	<ndll name="regexp" />
	<ndll name="zlib" />
	<ndll name="nme" haxelib="nme" />
	
</project>